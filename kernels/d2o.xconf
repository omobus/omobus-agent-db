<?xml version="1.0" encoding="utf-8" ?>
<kernel 
    module="kern_sync" 
    ldap.uri="ldaps://gateway.omobus.net:636" 
    ldap.bind_dn="uid=dummy,ou=distributors,dc=omobus,dc=local"
    ldap.bind_pw="0"
    ldap.base="uid=dummy,ou=distributors,dc=omobus,dc=local" 
    ldap.filter="(&amp;(objectClass=omobusMaintainPoint)(exchangeStatus=enabled))"
    Xldap.tls="yes"
    ftp.host="gateway.omobus.net"
    ftp.port="21021"
    ftp.path="d2o/"
    ftp.connect_timeout="30"
    ftp.recv_timeout="15"
    ftp.send_timeout="15"
    ftp.epsv="yes"
    ftp.tls="yes"
    ftp.ccc="no"
    ftp.cdc="no"
    pack.type="bz2"
    pack.level="9"
    Xpack.encoding="utf-8"
    cache="d2o/"
    diff="yes"
    Xthreads="2"
    >
<queries>
<xi:include href="../queries/d2o/TTD.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/accounts.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/account_params.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/account_prices.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/auto_orders.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/blacklist.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/constants.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/debts.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/discounts.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/erp_docs.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/erp_products.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/group_prices.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/mutuals_history_products.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/mutuals_history.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/mutuals.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/order_params.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/packs.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/permitted_returns.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/products.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/restrictions.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/sales_history.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/std_prices.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/stocks_history.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/symlinks.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/users.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/warehouses.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/wareh_stocks.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />
<xi:include href="../queries/d2o/warnings.xconf" xmlns:xi="http://www.w3.org/2003/XInclude" />

<!-- **** FILTERS: BEGIN **** -->
<int__accounts>
    <![CDATA[
select db_id person_id, t_id account_id from symlinks where obj_code='account' and t_id is not null and t_id <> ''
    order by 1, 2
    ]]>
</int__accounts>
<int__products>
    <![CDATA[
select db_id person_id, t_id prod_id from symlinks where obj_code='product' and t_id is not null and t_id <> ''
    order by 1, 2
    ]]>
</int__products>
<int__users>
    <![CDATA[
select db_id person_id, t_id user_id from symlinks where obj_code='user' and t_id is not null and t_id <> ''
    order by 1, 2
    ]]>
</int__users>
<!-- **** FILTERS: END **** -->
</queries>
<pack Xstrict="yes" Xtrace="yes">
    <TTD />
    <accounts />
    <constants />
    <order_params personalize="ErpCode" />
    <packs />
    <products />
    <symlinks personalize="ErpCode"/>
    <int__accounts personalize="ErpCode">
	<account_params key="account_id">
	    <group_prices key="group_price_id" />
	</account_params>
	<account_prices key="account_id" />
	<auto_orders key="account_id" />
	<blacklist key="account_id" />
	<debts key="account_id" />
	<discounts key="account_id" />
	<mutuals key="account_id" />
	<mutuals_history key="account_id">
	    <mutuals_history_products key="erp_id" />
	</mutuals_history>
	<restrictions key="account_id" />
	<sales_history key="account_id" />
    </int__accounts>
    <int__products personalize="ErpCode">
	<account_prices key="prod_id" />
	<auto_orders key="prod_id" />
	<blacklist key="prod_id" />
	<discounts key="prod_id" />
	<erp_products key="prod_id" />
	<group_prices key="prod_id" />
	<mutuals_history_products key="prod_id" />
	<restrictions key="prod_id" />
	<sales_history key="prod_id" />
	<std_prices key="prod_id" />
	<stocks_history key="prod_id" />
	<wareh_stocks key="prod_id" />
    </int__products>
    <int__users personalize="ErpCode">
	<users key="user_id" />
    </int__users>
    <erp_docs>
	<erp_products key="erp_id" />
    </erp_docs>
    <warehouses>
	<stocks_history key="wareh_id" />
	<wareh_stocks key="wareh_id" />
    </warehouses>
</pack>
</kernel>