<?xml version="1.0" encoding="utf-8"?>
<new_account_types templ="%new_account_type_id%;%descr%;">
<begin>
  <![CDATA[
update new_account_types set hidden=1
    where db_id = '%ErpCode%'
  ]]>
</begin>
<check_exist>
  <![CDATA[
select count(*) exist from new_account_types 
    where new_account_type_id = '%new_account_type_id%' and db_id = '%ErpCode%'
  ]]>
</check_exist>
<update>
  <![CDATA[
update new_account_types set descr='%descr%', hidden=0 
    where new_account_type_id = '%new_account_type_id%' and db_id = '%ErpCode%'
  ]]>
</update>
<insert>
  <![CDATA[
insert into new_account_types (db_id, new_account_type_id, descr, hidden) 
    values ('%ErpCode%', '%new_account_type_id%', '%descr%', 0)
  ]]>
</insert>
</new_account_types>
