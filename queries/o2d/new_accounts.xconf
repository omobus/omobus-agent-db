<?xml version="1.0" encoding="utf-8"?>
<new_accounts templ="%doc_id%;%fix_dt%;%doc_no%;%user_id%;%dev_login%;%new_account_type_id%;%doc_note%;%account%;%legal_address%;%address%;%number%;%attr_ids%;">
<check_exist>
  <![CDATA[
select count(*) exist from new_accounts 
    where doc_id = '%doc_id%' and db_id = '%ErpCode%'
  ]]>
</check_exist>
<insert>
  <![CDATA[
insert into new_accounts (db_id, doc_id, fix_dt, doc_no, user_id, dev_login, new_account_type_id, doc_note, account, address, legal_address, number, attr_ids) 
    values ('%ErpCode%', '%doc_id%', '%fix_dt%', '%doc_no%', '%user_id%', '%dev_login%', '%new_account_type_id%', '%doc_note%', '%account%', '%address%', '%legal_address%', 0%number%, '%attr_ids%')
  ]]>
</insert>
</new_accounts>
